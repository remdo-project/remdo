{
  admin off
}

:{$APP_PORT} {
  @options method OPTIONS
  handle @options {
    respond 204
  }

  route /doc* {
    uri strip_prefix /doc
    reverse_proxy 127.0.0.1:{env.YSWEET_PORT_INTERNAL}
  }

  handle_path /health {
    respond 200
  }

  root * /srv/remdo
  try_files {path} {path}/ /index.html
  file_server
}
