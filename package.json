{
  "name": "notes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hocuspocus/extension-logger": "^2.13.0",
    "@hocuspocus/provider": "^2.13.0",
    "@hocuspocus/server": "^2.13.0",
    "@lexical/list": "file:./lexical/packages/lexical-list",
    "@lexical/react": "file:./lexical/packages/lexical-react",
    "@lexical/utils": "file:./lexical/packages/lexical-utils",
    "@testing-library/react": "^16.1.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.9.1",
    "envalid": "^8.0.0",
    "fs-extra": "^11.2.0",
    "lexical": "file:./lexical/packages/lexical",
    "prettier": "^3.4.2",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.4",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^4.1.2",
    "react-router-dom": "^7.0.2",
    "uuid": "^11.0.3",
    "web-vitals": "^4.0.0",
    "y-indexeddb": "^9.0.9",
    "y-websocket": "^2.0.3"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "open": "./utils.sh open_document",
    "server": "VITE_DOCUMENTS=$(cd tests/data && ls *.json | sed 's/\\.json//' | tr '\n' ',') vite --host",
    "websocket-persist": "HOST=0.0.0.0 PORT=8080 YPERSISTENCE=./data/yjsDB npx y-websocket",
    "websocket": "HOST=0.0.0.0 PORT=8080 npx y-websocket",
    "hocuspocus": "node ./hocuspocus.mjs",
    "build": "vite build",
    "eject": "react-scripts eject",
    "test-browser": "npx playwright test --project=chromium",
    "test-browser-collab": "VITE_WS=true npm run test-browser",
    "test-browser-collab-watch": "VITE_WS=true npm run test-browser-watch",
    "test-browser-watch": "sh -c 'ARGS=\"$@\"; ./utils.sh watch_repo \"npm run test-browser -- $ARGS && echo \\\"\\x1b[32mDone, waiting for file changes...\\x1b[0m\\\"\"' --",
    "test-unit": "npx vitest run",
    "test-performance": "VITE_PERFORMANCE_TESTS=true VITE_LOG_LEVEL=info time -f 'Time: %es' npx vitest run performance/performance --reporter ./tests/unit/performance/reporter.ts -t",
    "test-unit-collab": "VITE_WS=true npx vitest run",
    "test-unit-watch-coverage": "npx vitest --ui --coverage.enabled --coverage.include=src/**",
    "test-unit-watch": "npx vitest --watch --ui",
    "test-unit-minimal": "npx vitest --reporter=./tests/unit/common/reporter.ts --watch --ui",
    "test": "npm run test-unit && npm run test-browser",
    "kill-npm-processes": "ps -eaf | grep 'npm run' | awk '{print $2}' |  xargs kill",
    "load": "./utils.sh run_serialization load",
    "save": "./utils.sh run_serialization save"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@lexical/eslint-plugin": "file:./lexical/packages/lexical-eslint-plugin",
    "@microlink/react-json-view": "^1.23.0",
    "@playwright/test": "^1.37.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/bootstrap": "^5.2.6",
    "@types/js-yaml": "^4.0.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "chokidar-cli": "^3.0.0",
    "esbuild": "^0.24.2",
    "eslint": "^8.57.0",
    "eslint-config-fbjs": "^4.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-ft-flow": "^2.0.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-lexical": "file:./lexical/eslint-plugin",
    "eslint-plugin-no-function-declare-after-return": "^1.1.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^9.0.11",
    "js-yaml": "^4.1.0",
    "jsdom": "^25.0.1",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-sort-json": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "1.77.6",
    "vite": "^6.0.5",
    "vitest": "^2.0.5",
    "vitest-preview": "^0.0.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
