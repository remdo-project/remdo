{
  "name": "notes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hocuspocus/extension-logger": "^3.2.3",
    "@hocuspocus/provider": "^3.2.3",
    "@hocuspocus/server": "^3.2.3",
    "@lexical/link": "file:./lexical/packages/lexical-link",
    "@lexical/list": "file:./lexical/packages/lexical-list",
    "@lexical/react": "file:./lexical/packages/lexical-react",
    "@lexical/utils": "file:./lexical/packages/lexical-utils",
    "@lexical/yjs": "file:./lexical/packages/lexical-yjs",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.9.1",
    "dotenv": "^17.2.1",
    "envalid": "^8.0.0",
    "fs-extra": "^11.2.0",
    "lexical": "file:./lexical/packages/lexical",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.4",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^4.1.2",
    "react-router-dom": "^7.9.1",
    "uuid": "^13.0.0",
    "y-indexeddb": "^9.0.9",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.0"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "open": "./utils.sh open_document",
    "server": "VITE_DOCUMENTS=$(cd tests/data && ls *.json | sed 's/\\.json//' | tr '\n' ',') vite --host",
    "websocket-persist": "HOST=0.0.0.0 PORT=8080 YPERSISTENCE=./data/yjsDB npx y-websocket",
    "websocket": "HOST=0.0.0.0 PORT=8080 npx y-websocket",
    "hocuspocus": "node ./hocuspocus.mjs",
    "build": "vite build",
    "test-browser": "npx playwright test --project=chromium",
    "test-browser-collab": "FORCE_WEBSOCKET=true npm run test-browser",
    "test-browser-collab-watch": "FORCE_WEBSOCKET=true npm run test-browser-watch",
    "test-browser-watch": "sh -c 'ARGS=\"$@\"; ./utils.sh watch_repo \"npm run test-browser -- $ARGS && echo \\\"\\x1b[32mDone, waiting for file changes...\\x1b[0m\\\"\"' --",
    "test-unit": "npx vitest run",
    "test-performance": "VITE_PERFORMANCE_TESTS=true VITE_LOG_LEVEL=info time -f 'Time: %es' npx vitest run performance/performance --reporter ./tests/unit/performance/reporter.ts -t",
    "test-unit-collab": "FORCE_WEBSOCKET=true npx vitest run --maxWorkers=1",
    "test-unit-watch-coverage": "npx vitest --ui --coverage.enabled --coverage.include=src/**",
    "test-unit-watch": "npx vitest --watch --ui",
    "test-unit-minimal": "npx vitest --reporter=./tests/unit/common/reporter.ts --watch --ui",
    "test": "npm run test-unit && npm run test-browser",
    "kill-npm-processes": "ps -eaf | grep 'npm run' | awk '{print $2}' |  xargs kill",
    "load": "./utils.sh run_serialization load",
    "save": "./utils.sh run_serialization save",
    "submodules": "git submodule update --init --recursive"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@microlink/react-json-view": "^1.27.0",
    "@playwright/test": "^1.37.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/bootstrap": "^5.2.6",
    "@types/js-yaml": "^4.0.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "@vitest/ui": "^3.2.4",
    "@y/websocket-server": "^0.1.1",
    "@testing-library/react": "^16.1.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "husky": "^9.0.11",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-sort-json": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "1.92.1",
    "typescript": "^5.9.0",
    "vite": "^7.1.3",
    "vitest": "^3.0.0",
    "vitest-preview": "^0.0.1"
  },
  "packageManager": "npm@10.9.3"
}
