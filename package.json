{
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd",
  "engines": {
    "node": ">=22.0.0"
  },
  "browserslist": {
    "production": [
      "last 2 Chrome versions",
      "last 2 ChromeAndroid versions",
      "last 2 Edge versions",
      "last 2 Firefox versions",
      "last 2 Safari versions"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  },
  "remdoSupport": {
    "tools": {
      "pnpm": ">=10.18.0",
      "git": ">=2.40.0"
    },
    "notes": [
      "Browsers outside the Browserslist ranges are unsupported.",
      "Node runtimes must satisfy engines.node."
    ]
  },
  "scripts": {
    "dev": "concurrently \"pnpm run dev:web\" \"pnpm run dev:ws\" \"pnpm run test:preview\"",
    "dev:web": "vite",
    "dev:ws": "tsx ./tools/collab-server.ts",
    "dev:init": "./tools/dev-init.sh",
    "build": "vite build",
    "preview": "vite preview",
    "load": "tsx ./tools/snapshot.ts load",
    "lint": "cross-env CI=1 pnpm run typecheck && pnpm run lint:code",
    "lint:code": "cross-env CI=1 eslint --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
    "lint:code:watch": "chokidar \"**/*.{ts,tsx,js,jsx,mts,cts,json}\" --initial -i \"**/{node_modules,data,.git}/**\" -d 150 -c \"pnpm lint:code && echo LINT_OK || { echo LINT_FAIL; exit 1; }\"",
    "lint:fix": "eslint --max-warnings=0 --fix --cache --cache-location node_modules/.cache/eslint",
    "typecheck": "tsc",
    "typecheck:watch": "pnpm run typecheck --watch",
    "format": "pnpm run lint:fix",
    "check": "pnpm run lint && pnpm run test",
    "deps:list": "pnpm list --depth=-1 --silent",
    "test": "pnpm run test:unit",
    "test:unit": "COLLAB_ENABLED=false pnpm exec vitest run",
    "test:unit:collab": "pnpm exec vitest run",
    "test:unit:coverage": "COLLAB_ENABLED=false pnpm exec vitest run --coverage",
    "test:unit:watch": "COLLAB_ENABLED=false tsx ./tools/test-unit-watch.ts",
    "test:preview": "tsx ./tools/test-preview.ts",
    "save": "tsx ./tools/snapshot.ts save"
  },
  "dependencies": {
    "@lexical/devtools-core": "^0.38.2",
    "@lexical/history": "^0.38.2",
    "@lexical/list": "^0.38.2",
    "@lexical/markdown": "^0.38.2",
    "@lexical/react": "^0.38.2",
    "@lexical/rich-text": "^0.38.2",
    "@lexical/utils": "^0.38.2",
    "@lexical/yjs": "^0.38.2",
    "@mantine/core": "^8.3.6",
    "@mantine/hooks": "^8.3.6",
    "@y-sweet/client": "^0.9.1",
    "@y-sweet/react": "^0.9.1",
    "@y-sweet/sdk": "^0.9.1",
    "lexical": "^0.38.2",
    "lib0": "^0.2.114",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.27"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@eslint-react/eslint-plugin": "^2.3.5",
    "@lexical/eslint-plugin": "^0.38.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/estree": "^1.0.8",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.5.12",
    "@vitejs/plugin-react-swc": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "dotenv-flow": "4.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-unicorn": "^62.0.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.6",
    "vitest-preview": "^0.0.3",
    "ws": "8.18.3",
    "y-sweet": "^0.9.1"
  },
  "pnpm": {
    "overrides": {
      "js-yaml": "4.1.1"
    },
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "y-sweet"
    ]
  }
}
