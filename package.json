{
  "name": "notes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@lexical/list": "file:./lexical/packages/lexical-list",
    "@lexical/react": "file:./lexical/packages/lexical-react",
    "@lexical/utils": "file:./lexical/packages/lexical-utils",
    "@playwright/test": "^1.29.2",
    "@testing-library/react": "^13.3.0",
    "bootstrap": "git+https://github.com/twbs/bootstrap.git#v5.3.0-alpha3",
    "bootstrap-icons": "^1.9.1",
    "lexical": "file:./lexical/packages/lexical",
    "playwright": "^1.29.2",
    "prettier": "^2.8.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4",
    "y-indexeddb": "^9.0.9",
    "y-websocket": "^1.4.4",
    "yjs": "^13.6.7"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "open": "./utils.sh open_document",
    "server": "VITE_DOCUMENTS=$(ls tests/data | sed 's/\\.json//' | tr '\n' ',') vite --host",
    "websocket-persist": "HOST=0.0.0.0 PORT=8080 YPERSISTENCE=./data/yjsDB npx y-websocket",
    "websocket": "HOST=0.0.0.0 PORT=8080 npx y-websocket",
    "build": "vite build",
    "eject": "react-scripts eject",
    "test-browser": "VITE_DISABLECOLLAB=true npm run test-browser-collab",
    "test-browser-collab": "npx playwright test --project=chromium",
    "test-browser-watch": "VITE_DISABLECOLLAB=true ./utils.sh watch_repo 'npm run test-browser && echo \"\\x1b[32mDone, waiting for file changes...\\x1b[0m\"'",
    "test-unit": "VITE_DISABLECOLLAB=true npx vitest run",
    "test-performance": "VITE_PERFORMANCE_TESTS=true npx vitest run performance -t",
    "test-unit-collab": "npx vitest run",
    "test-unit-watch": "VITE_DISABLECOLLAB=true npx vitest --ui",
    "test": "npm run test-unit && npm run test-browser",
    "kill-npm-processes": "ps -eaf | grep 'npm run' | awk '{print $2}' |  xargs kill",
    "load": "./utils.sh run_serialization load",
    "save": "./utils.sh run_serialization save"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.30.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/bootstrap": "^5.2.6",
    "@types/js-yaml": "^4.0.5",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitest/ui": "^0.28.4",
    "chokidar-cli": "^3.0.0",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.1",
    "js-yaml": "^4.1.0",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "prettier-plugin-sort-json": "^1.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.59.2",
    "vite": "^4.1.1",
    "vite-plugin-terminal": "^1.1.0",
    "vitest": "^0.28.4",
    "vitest-preview": "^0.0.1"
  }
}
